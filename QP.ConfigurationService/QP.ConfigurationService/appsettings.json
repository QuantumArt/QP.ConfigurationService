{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ConfigMonitoringTimeout": 15,
  "AllowedHosts": "*",
  "QpConfigurationPath": "__ConfigurationPath__",
  "Jwt": {
    "Scheme": "QP.ConfigurationService.AuthenticationScheme",
    "Issuer": "QP.ConfigurationService.Issuer",
    "Audience": "QP.ConfigurationService.Audience",
    "Secret": "__ConfigurationToken__"
  }
}
